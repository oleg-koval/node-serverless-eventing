{
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
    },
    "rules": {
      "prettier/prettier": ["error", {
        "tabWidth": 4,
        "printWidth": 100,
        "semi": false,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "parser": "flow",
        "singleQuote": true
      }]
    },
    "extends": [
      "eslint:recommended"
    ],
      "env": {
          "shared-node-browser": true,
          "node": true,
          "es6": true,
          "jest": true
      }
  }
